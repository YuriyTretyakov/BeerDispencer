{

  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd-MM-yyyy HH:mm:ss.fff} {Level:u3}] <{SourceContext}> {Message:lj} {NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "BeerDispencer.log",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:dd-MM-yyyy HH:mm:ss.fff} {Level:u3}] <{SourceContext}> {Message:lj} {NewLine}{Exception}"
        }
      }
    ]
  }
}